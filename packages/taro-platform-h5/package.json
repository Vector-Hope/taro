{
  "name": "@tarojs/plugin-platform-h5",
  "version": "3.6.7",
  "description": "Web 端平台插件",
  "author": "ZakaryCode",
  "license": "MIT",
  "main": "index.js",
  "keywords": [
    "taro"
  ],
  "files": [
    "dist",
    "index.js",
    "types"
  ],
  "sideEffects": [
    "./dist/runtime/index.js",
    "*.scss",
    "*.css"
  ],
  "types": "types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NervJS/taro.git"
  },
  "scripts": {
    "prebuild": "run-s clean generate:desc",
    "build": "cross-env NODE_ENV=production pnpm run rollup",
    "dev": "cross-env NODE_ENV=development pnpm run rollup -w",
    "rollup": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "generate:desc": "ts-node -r tsconfig-paths/register --project ./build/tsconfig.json --files ./build/parse-definition-json",
    "clean": "rimraf dist"
  },
  "bugs": {
    "url": "https://github.com/NervJS/taro/issues"
  },
  "dependencies": {
    "@tarojs/service": "workspace:*",
    "@tarojs/components": "workspace:*",
    "@tarojs/components-react": "workspace:*",
    "@tarojs/router": "workspace:*",
    "@tarojs/shared": "workspace:*",
    "@tarojs/taro-h5": "workspace:*",
    "babel-plugin-transform-taroapi": "workspace:*",
    "lodash-es": "4.17.21",
    "resolve": "^1.22.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@tarojs/taro": "workspace:*",
    "change-case": "^4.1.2",
    "rollup": "^3.8.1",
    "rollup-plugin-node-externals": "^5.0.0",
    "rollup-plugin-ts": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.7.4"
  }
}
